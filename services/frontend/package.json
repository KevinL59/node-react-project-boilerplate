{
  "name": "react-typescript-boilerplate",
  "version": "0.0.1",
  "description": "React project boilerplate using typescript",
  "main": "index.tsx",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --watch --progress --hot --inline --port 4200",
    "build-dev": "rm -rf ./dist/* && webpack"
  },
  "keywords": [
    "React",
    "Typescript"
  ],
  "author": "Kevin Letupe",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-logger": "^3.0.6",
    "eslint": "^5.11.1",
    "eslint-plugin-react": "^7.12.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^3.2.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.10",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx",
      "scss"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/tests/__mocks__/styleMock.ts"
    }
  }
}
